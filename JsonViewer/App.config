            <setting name="HighlightSelectedParents" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="FindText" serializeAs="String">
                <value />
            </setting>
            <setting name="MainWindowToolbarTextVisible" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="MainWindowToolbarIconVisible" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="MainWindowWarnOnDefaultConfig" serializeAs="String">
