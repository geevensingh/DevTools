        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
    </startup>
    <userSettings>
        <JsonViewer.Properties.Settings>
            <setting name="MainWindowPlacement" serializeAs="String">
                <value />
